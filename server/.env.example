NODE_ENV=development
PORT=3000
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/?appName=suimo

# OAuth / receipts (Ed25519 JWT)
ED25519_PRIVATE_KEY_PATH=./keys/oauth_private_key.pem
OAUTH_ISSUER=https://suimo.onrender.com
OAUTH_RESOURCE=https://suimo.onrender.com/mcp

# Secrets
JWT_SECRET=change-me-min-32-chars-base64-secret
KEY_ENCRYPTION_KEY=change-me-min-32-chars-base64-secret

# Client App
CLIENT_APP_URL=http://localhost:5173
CLIENT_AUTH_PATH=/auth

# Sui (payouts)
SUI_RPC_URL=https://testnet-rpc.sui.chainbase.online
SUI_PLATFORM_PRIVATE_KEY=suiprivkey1...
SUI_USDC_TYPE=0xa1ec7fc00a6f40db9693ad1415d0c193ad3906494428cf252621037bd7117e29::usdc::USDC
SUI_USDC_DECIMALS=6
SUI_PAYTO=0x...

# Walrus + Seal
WALRUS_NETWORK=testnet
WALRUS_RELAY_URL=https://upload-relay.testnet.walrus.space
SEAL_POLICY_PACKAGE=0x...
WAL_COIN_TYPE=0x8270feb7375eee355e64fdb69c50abb6b5f9393a722883c1cf45f8e26048810a::wal::WAL
WAL_DECIMALS=9

# Session cookies
SESSION_COOKIE_NAME=suimo_session
SESSION_COOKIE_DOMAIN=
SESSION_COOKIE_SECURE=false
SESSION_COOKIE_MAX_AGE=604800

# Keep-alive (pings server every 10 min to prevent Render spin-down)
SERVER_URL=https://your-app.onrender.com
ENABLE_KEEPALIVE=true
